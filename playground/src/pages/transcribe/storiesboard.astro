---
import { StoriesBoard } from "../../components/transcribe/story/StoriesBoard";
import { getConfig, validateConfig } from "../../utils/core/config";
import Layout from "../../layouts/Layout.astro";

const config = await getConfig();
const validation = await validateConfig(config);
if (!validation.isValid || !config) {
  return Astro.redirect("/storykeep/init");
}
---
<Layout title="Transcribes - Stories Board" pubDatetime={new Date()} modDatetime={new Date()} config={config}>
  <StoriesBoard client:visible/>
</Layout>