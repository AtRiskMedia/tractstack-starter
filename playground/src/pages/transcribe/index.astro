---
import { TranscribesDashboard } from "../../components/transcribe/dashboard/TranscribesDashboard";
import { getConfig, validateConfig } from "../../utils/core/config";
import Layout from "../../layouts/Layout.astro";

const config = await getConfig(Astro.locals.tenant?.paths.configPath);
const validation = await validateConfig(config);
if (!validation.isValid || !config) {
  return Astro.redirect("/storykeep/init");
}
---

<Layout title="Transcribes" pubDatetime={new Date()} modDatetime={new Date()} config={config}>
  <TranscribesDashboard client:visible />
</Layout>
